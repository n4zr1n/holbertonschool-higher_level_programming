"""
Doctests for the MyList class to check various edge cases and behaviors.

>>> my_list = MyList()
>>> isinstance(my_list, list)
True

>>> print(my_list)
[]

>>> my_list.append(10)
>>> my_list.append(2)
>>> my_list.append(30)
>>> print(my_list)
[10, 2, 30]

>>> my_list.print_sorted()
[2, 10, 30]
>>> print(my_list)
[10, 2, 30]

>>> my_list = MyList([-5, -1, -3])
>>> my_list.print_sorted()
[-5, -3, -1]

>>> my_list = MyList()
>>> my_list.print_sorted()
[]

>>> my_list = MyList([3, 1, 2])
>>> sorted_copy = sorted(my_list)
>>> my_list.print_sorted()
[1, 2, 3]
>>> print(my_list == [3, 1, 2])  # original list unchanged
True
>>> print(sorted_copy)
[1, 2, 3]
"""
